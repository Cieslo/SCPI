<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="items.xsd">
	<itemtypes>
		<itemtype code="IBaseComponent" autocreate="false">
			<attributes>
				<attribute type="java.lang.string" qualifier="rawComponentData">
					<description>Concatenated ObjectType and ProductObject codes</description>
					<modifiers read="true" write="true" search="true"
							optional="true" />
					<persistence type="property" />
				</attribute>
			</attributes>
		</itemtype>
		<itemtype code="IBasePartner" autocreate="false">
			<attributes>
				<attribute type="java.lang.string" qualifier="rawPartnerId">
					<description>Raw string of partner to be mapped to b2bunit/customer</description>
					<modifiers read="true" write="true" search="true"
							optional="true" />
					<persistence type="property" />
				</attribute>
			</attributes>
		</itemtype>
		
		<itemtype code="IBaseStructure" autocreate="false">
			<attributes>
				<attribute type="java.lang.string" qualifier="rawObjectGuids">
					<description>Concatenated GUIDs of objects of component, parent and root</description>
					<modifiers read="true" write="true" search="true"
							optional="true" />
					<persistence type="property" />
				</attribute>
			</attributes>
		</itemtype>
	</itemtypes>
</items>
