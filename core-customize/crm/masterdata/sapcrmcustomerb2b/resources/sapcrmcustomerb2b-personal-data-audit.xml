<?xml version="1.0" encoding="UTF-8"?>

<audit-report-config name="PersonalDataReport">
	<given-root-type>User</given-root-type>
	<types>
		<type code="UserGroup" mode="append">
			<virtual-attributes>
				<virtual-attribute expression="b2bunit" type="SAPPaymentInfo"
					many="true" displayName="SAPPaymentInfos" />
			</virtual-attributes>
		</type>
		<type code="SAPPaymentInfo">
			<atomic-attributes>
				<atomic-attribute qualifier="validFrom" displayName="ValidFrom" />
				<atomic-attribute qualifier="validTo" displayName="ValidTo" />
				<!-- SAPCCPaymentInfo -->
				<atomic-attribute qualifier="ccOwner" displayName="CcOwner" />
				<atomic-attribute qualifier="cardId" displayName="CardId" />
				<atomic-attribute qualifier="cardNumber" displayName="CardNumber" />
				<atomic-attribute qualifier="issuer" displayName="Issuer" />
				<atomic-attribute qualifier="issuingDate" displayName="IssuingDate" />
				<atomic-attribute qualifier="subscriptionId" displayName="SubscriptionId" />
			</atomic-attributes>
		</type>
	</types>
</audit-report-config>