	<context component="create-wizard" type="BusinessAgreementHeader"
		merge-by="type">
		<wz:flow xmlns:wz="http://www.hybris.com/cockpitng/config/wizard-config"
			id="BusinessAGHeaderWizard" title="create.businessAG.title">
			<wz:prepare id="newBusinessAGHeader">
				<wz:initialize property="newBusinessAGHeader" type="ctx.TYPE_CODE" />
			</wz:prepare>

			<wz:step id="step1" label="create.businessAGheader.general.label"
				sublabel="create.businessAG.general.sublabel">
				<wz:content id="step1.content">
					<wz:property-list root="newBusinessAGHeader">
						<wz:property qualifier="businessagreementdefault"
							type="java.lang.Boolean" />
						<wz:property qualifier="businessagreementclass"
							type="Reference(CRMBusinessAgreementClass)" editor="com.hybris.cockpitng.editor.defaultreferenceeditor" />
						<wz:property qualifier="bankidinc" type="java.lang.String" />
						<wz:property qualifier="bankidout" type="java.lang.String" />
						<wz:property qualifier="creditcardinc" type="java.lang.String" />
						<wz:property qualifier="creditcardout" type="java.lang.String" />
						<wz:property qualifier="addresscorrespondenceguid"
							type="java.lang.String" />
						<wz:property qualifier="addressdunningguid" type="java.lang.String" />
						<wz:property qualifier="addresspayerguid" type="java.lang.String" />
						<wz:property qualifier="addressrecipientguid" type="java.lang.String" />
						<wz:property qualifier="addressirguid" type="java.lang.String" />
						<wz:property qualifier="addresstaxguid" type="java.lang.String" />
						<wz:property qualifier="businessagreementpayer" type="java.lang.String" />
						<wz:property qualifier="taxcategory"
							type="Reference(CRMBusinessAgreementTaxCategory)" editor="com.hybris.cockpitng.editor.defaultreferenceeditor" />
						<wz:property qualifier="taxcode"
							type="Reference(CRMBusinessAgreementTaxCode)" editor="com.hybris.cockpitng.editor.defaultreferenceeditor" />
						<wz:property qualifier="refnumber" type="java.lang.String" />
						<wz:property qualifier="businessagreement" type="Reference(BusinessAgreement)"
							editor="com.hybris.cockpitng.editor.defaultreferenceeditor" />
						<wz:property qualifier="validto" type="java.lang.String" />

					</wz:property-list>
				</wz:content>
				<wz:navigation id="step1.navigation">
					<wz:cancel />

					<wz:done>
						<wz:save property="newBusinessAGHeader" />
					</wz:done>
				</wz:navigation>
			</wz:step>


		</wz:flow>
	</context>



