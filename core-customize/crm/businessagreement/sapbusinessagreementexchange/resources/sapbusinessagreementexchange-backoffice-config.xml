	<context component="create-wizard" type="BusinessAgreement"
		merge-by="type">
		<wz:flow xmlns:wz="http://www.hybris.com/cockpitng/config/wizard-config"
			id="BusinessAGWizard" title="create.businessAG.title">
			<wz:prepare id="newBusinessAG">
				<wz:initialize property="newBusinessAG" type="ctx.TYPE_CODE" />
			</wz:prepare>

			<wz:step id="step1" label="create.businessAG.general.label"
				sublabel="create.businessAG.general.sublabel">
				<wz:content id="step1.content">
					<wz:property-list root="newBusinessAG">
						<wz:property qualifier="guid" type="java.lang.String" />
						<wz:property qualifier="id" type="java.lang.String" />
						<wz:property qualifier="text" type="java.lang.String" />
