							editor="com.hybris.cockpitng.editor.defaultreferenceeditor" />

					</wz:property-list>
				</wz:content>

				<wz:navigation id="step1.navigation">
					<wz:cancel />
					<wz:next visible="!#empty(newBusinessAG.guid)" />
					<wz:done>
						<wz:save property="newBusinessAG" />
					</wz:done>
				</wz:navigation>
			</wz:step>


		</wz:flow>
	</context>


	<!-- Business Agreement Header -->



