	<bean
		class="com.sap.hybris.sapbusinessagreementfacades.data.BusinessAgreementData">
		<property name="businessAgreementID" type="String" />
		<property name="description" type="String" />
		<property name="businessAgreementClass" type="String" />
		<property name="isDefault" type="java.lang.Boolean" />
	</bean>

	<bean
		class="com.sap.hybris.sapbusinessagreementfacades.data.BusinessAgreementDetailsData"
		extends="com.sap.hybris.sapbusinessagreementfacades.data.BusinessAgreementData">

		<property name="partnerName" type="String" />
		<property name="refnumber" type="String" />
		<property name="collectivebill" type="java.lang.Boolean" />
		<!-- <property name="partnerAddress" type="de.hybris.platform.commercefacades.user.data.AddressData>" 
			/> -->
		<!-- <property name="partnerClass" type="String>" /> -->

		<property name="addressData"
			type="de.hybris.platform.commercefacades.user.data.AddressData" />

		<property name="paymentVia"
			type="com.sap.hybris.sapbusinessagreementfacades.data.BusinessAgreementPaymentMethod" />

		<property name="refundVia"
			type="com.sap.hybris.sapbusinessagreementfacades.data.BusinessAgreementPaymentMethod" />

		<property name="correspondenceRecipientId" type="String" />
		<property name="correspondenceRecipientDesc" type="String" />
		<property name="correspondenceVariant" type="String" />
		<property name="correspondenceShipControl" type="String" />

		<property name="dunningRecipientId" type="String" />
		<property name="dunningDesc" type="String" />
		<property name="dunningAddress"
			type="de.hybris.platform.commercefacades.user.data.AddressData" />
		<property name="dunningShipControl" type="String" />
		
		<property name="taxType" type="String" />
		<property name="taxCode" type="String" />
		<property name="taxRelevantAddress" type="de.hybris.platform.commercefacades.user.data.AddressData" />

		<property name="payerCreditCardInfo"
			type="com.sap.hybris.sapbusinessagreementfacades.data.CreditCardData" />
		<property name="recipientCreditCardInfo"
			type="com.sap.hybris.sapbusinessagreementfacades.data.CreditCardData" />
		<property name="payerBankDetailsInfo"
			type="com.sap.hybris.sapbusinessagreementfacades.data.BankDetailsData" />
		<property name="recipientBankDetailsInfo"
			type="com.sap.hybris.sapbusinessagreementfacades.data.BankDetailsData" />
			
		<property name="b2cCustomerPayerCCInfo"
			type="de.hybris.platform.commercefacades.order.data.CCPaymentInfoData" />		
		<property name="b2cCustomerRecipientCCInfo"
			type="de.hybris.platform.commercefacades.order.data.CCPaymentInfoData" />
