<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="items.xsd">

	<collectiontypes>
		<collectiontype elementtype="AbstractOrderEntry" code="AssociatedOrderEntryList"
			type="list" autocreate="true" />
	</collectiontypes>
	<itemtypes>
		<itemtype autocreate="false" code="CsTicket" generate="false"
			singleton="true">
			<attributes>
				<attribute type="AssociatedOrderEntryList" qualifier="associatedOrderEntries">
					<persistence type="property" />
					<modifiers unique="false" optional="true" read="true"
						write="true" search="true" />
				</attribute>
			</attributes>
		</itemtype>
		<itemtype code="SAPConfiguration" autocreate="false"
			generate="false">
			<attributes>
				<attribute qualifier="comp_serviceOrg" generate="true"
					autocreate="true" type="java.lang.String">
					<description>Service org</description>
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute qualifier="comp_serviceOrgResp" generate="true"
					autocreate="true" type="java.lang.String">
					<description>Service org Responsible</description>
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute qualifier="comp_processType" generate="true"
					autocreate="true" type="java.lang.String">
					<description>Service Transaction Type</description>
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute qualifier="comp_servOrgShort" generate="true"
					autocreate="true" type="java.lang.String">
					<description>Service Org Short</description>
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute qualifier="complaintCatagoryCatalogType"
					generate="true" autocreate="true" type="java.lang.String">
					<description>Category Catalog name for Complaint</description>
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute qualifier="complaintCatagoryCodeGroup"
					generate="true" autocreate="true" type="java.lang.String">
					<description>Crm schema id for Complaint</description>
					<modifiers read="true" write="true" search="true"
						optional="true" />
					<persistence type="property" />
				</attribute>
			</attributes>
		</itemtype>
	</itemtypes>
</items>
